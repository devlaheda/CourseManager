pipeline {
    agent any
    stages {
        stage('Restore') {
            steps {
                sh 'dotnet --version'
            }
        }
        stage('Build') {
            steps {
                sh 'dotnet --version'
            }
        }
        stage('Test') {
            steps {
                sh 'dotnet --version'
            }
        }
        stage('Publish') {
            steps {
                sh 'dotnet --version'
            }
        }
    }
}
